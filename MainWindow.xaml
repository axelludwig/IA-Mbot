<Window x:Class="IA_Mbot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IA_Mbot"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Name="StartStopButton" Content="Button" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="StartStopButton_Click"/>
        <ComboBox Name="comboBoxWindows" HorizontalAlignment="Center" Margin="0,8,0,0" VerticalAlignment="Top" Width="656" SelectionChanged="ComboBox_SelectionChanged"/>
        <Image Name="imageScreenshot" HorizontalAlignment="Left" Height="355" Margin="10,62,0,0" VerticalAlignment="Top" Width="785"/>

    </Grid>
</Window>
